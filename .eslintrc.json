{
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"plugins": ["react"],
	"rules": {
		"react/prop-types": 0,
		"react/no-unescaped-entities": 0,
		"react/react-in-jsx-scope": 0
	},
	"parserOptions": {
		"ecmaVersion": 2022,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
